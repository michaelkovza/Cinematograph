.contacts {
  
  &__contacts {
    text-align: left;
    max-width: 902px;
    margin: 0 auto;
    padding-top: 54px;
  }

  &__content-container {
    padding-bottom: 15px;
  }
  
  &__content {
    font-family: var(--primary-font);
    font-size: 14px;
    line-height: 1.3;
    color: var(--primary-text-color);
  }

  &__link {
    color: var(--secondary-accent-color);
  }
}