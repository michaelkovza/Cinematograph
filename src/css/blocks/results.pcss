.results {
  &__list {
    text-align: left;
    max-width: 902px;
    margin: 0 auto;
    padding-top: 54px;
  }

  &__item {
    padding-bottom: 25px;
  }

  &__search-title {
    font-family: var(--primary-font);
    font-size: 14px;
    color: var(--primary-text-color);
    font-weight: normal;
  }

  &__link {
    font-family: var(--primary-font);
    font-size: 14px;
    color: var(--secondary-accent-color);
    font-weight: normal;
  }

}
